{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/nestjs-monorepo/src",
  "monorepo": true,
  "root": "apps/nestjs-monorepo",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/nestjs-monorepo/tsconfig.app.json"
  },
  "projects": {
    "nestjs-monorepo": {
      "type": "application",
      "root": "apps/nestjs-monorepo",
      "entryFile": "main",
      "sourceRoot": "apps/nestjs-monorepo/src",
      "compilerOptions": {
        "tsConfigPath": "apps/nestjs-monorepo/tsconfig.app.json"
      }
    },
    "user-api": {
      "type": "application",
      "root": "apps/user-api",
      "entryFile": "main",
      "sourceRoot": "apps/user-api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user-api/tsconfig.app.json"
      }
    },
    "entity": {
      "type": "library",
      "root": "libs/entity",
      "entryFile": "index",
      "sourceRoot": "libs/entity/src",
      "compilerOptions": {
        "tsConfigPath": "libs/entity/tsconfig.lib.json"
      }
    }
  }
}